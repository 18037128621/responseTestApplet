image:first-of-type {
  width: 750rpx;
  height: 1400rpx;
}
.init {
  position: absolute;
  top: 30%;
  left: 20%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
}
.init image {
  width: 250rpx;
  height: 250rpx;
}
.init view:first-of-type {
  margin-top: 70rpx;
  font-size: 60rpx;
}
.start {
  position: absolute;
  top: 0%;
  left: 28%;
}
.start view:first-of-type {
  margin-top: 300px;
  font-size: 30px;
}
.start image {
  position: fixed;
  width: 100px;
  height: 100px;
}
.start .result {
  position: fixed;
  text-align: center;
  margin: -300rpx 20rpx;
}
.start .result .resurrection {
  position: relative;
  top: 285rpx;
  left: -160rpx;
  width: 260rpx;
  background-color: #87f70e;
}
.start .result .reset {
  position: relative;
  top: 190rpx;
  left: 170rpx;
  width: 220rpx;
  background-color: #f7dd48;
}

