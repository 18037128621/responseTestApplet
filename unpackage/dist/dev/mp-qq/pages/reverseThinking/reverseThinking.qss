.background {
  width: 750rpx;
  height: 1400rpx;
  background-color: #aaaaff;
}
.init {
  position: fixed;
  top: 15%;
  left: 10%;
  width: 600rpx;
  height: 950rpx;
}
.init .icon {
  position: relative;
  left: 25%;
  width: 300rpx;
  height: 300rpx;
}
.init .content {
  margin-top: 70rpx;
  font-size: 40rpx;
  text-align: center;
  color: white;
}
.init .content view {
  margin-top: 30rpx;
}
.init .content text {
  position: absolute;
  top: 75%;
  left: 23%;
}
.startPlay_box {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.startPlay_box canvas {
  position: absolute;
  top: 10%;
  left: 27%;
  width: 350rpx;
  height: 350rpx;
}
.startPlay_box .time-schedule {
  position: relative;
  top: 550rpx;
}
.startPlay_box .time-schedule .sum-schedule {
  width: 500rpx;
  height: 20rpx;
  background-color: white;
  border-radius: 20rpx;
  margin: 0rpx 50rpx;
}
.startPlay_box .time-schedule .current-schedule {
  position: relative;
  top: -20rpx;
  height: 20rpx;
  background-color: #ffaa00;
  border-radius: 20rpx;
  margin: 0rpx 50rpx;
}
.startPlay_box .time-num {
  position: absolute;
  top: 600rpx;
}
.startPlay_box text {
  position: absolute;
  top: 60%;
  font-size: 60rpx;
  font-weight: bold;
  color: white;
}
.startPlay_box .buttons {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: space-around;
          justify-content: space-around;
  position: absolute;
  top: 75%;
  width: 750rpx;
}
.startPlay_box .buttons view {
  width: 100rpx;
  height: 100rpx;
  font-size: 50rpx;
  border-radius: 50%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  border: 5rpx solid white;
}
.startPlay_box .buttons .yes_btn {
  background-color: #00ff00;
}
.startPlay_box .buttons .no_btn {
  background-color: #ff557f;
}
.result {
  position: absolute;
  top: 30%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
}
.result view:first-of-type {
  font-size: 70rpx;
}
.result view:nth-of-type(2) {
  margin-top: 30rpx;
  font-size: 40rpx;
}
.result button {
  width: 250rpx;
  height: 100rpx;
  font-size: 35rpx;
  border: 3px solid white;
  color: white;
}
.result .continue {
  margin-top: 200rpx;
  background-color: #55ffff;
}
.result .buttons {
  position: absolute;
  top: 400rpx;
  left: -230rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.result .buttons .resurrection {
  width: 250rpx;
  background-color: #87f70e;
}
.result .buttons .reset {
  width: 220rpx;
  background-color: #f7dd48;
}

