image:first-of-type {
  width: 750rpx;
  height: 1400rpx;
}
.init {
  position: absolute;
  top: 30%;
  left: 20%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
}
.init image {
  width: 250rpx;
  height: 250rpx;
}
.init view:first-of-type {
  margin-top: 70rpx;
  font-size: 60rpx;
}
.start {
  position: absolute;
  top: 0%;
  left: 220rpx;
  width: 600rpx;
}
.start view:first-of-type {
  margin-top: 300px;
  font-size: 30px;
}
.start image {
  position: fixed;
  width: 100px;
  height: 100px;
}
.start .result {
  position: absolute;
  top: -450rpx;
  left: -150rpx;
  width: 600rpx;
  text-align: center;
}
.start .result .continue {
  position: absolute;
  left: 200rpx;
  margin-top: 300rpx;
  background-color: #4CD964;
}
.start .result .resurrection {
  position: absolute;
  left: 0rpx;
  margin-top: 300rpx;
  background-color: #87f70e;
}
.start .result .reset {
  position: absolute;
  left: 360rpx;
  margin-top: 300rpx;
  background-color: #f7dd48;
}

